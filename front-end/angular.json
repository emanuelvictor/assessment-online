            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/assets/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/materialize-css/dist/css/materialize.css",
              "src/assets/styles/font.css",
              "src/assets/styles/gallery-materialize.css",
              "src/assets/styles/parallax-style.css",
              "src/assets/styles/emanuelvictor-gallery-materialize.scss",
              "src/assets/styles/google-maps.iframe.scss",
              "src/site/application/site-theme.scss"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.js",
              "./node_modules/hammerjs/hammer.js",
              "./node_modules/materialize-css/dist/js/materialize.js",
              "src/assets/scripts/emanuelvictor-gallery-materialize.js",
              "src/assets/scripts/init.js",
              "src/assets/scripts/galleryExpand.js",
              "src/assets/scripts/smtp.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
