{
  "name": "meavalia",
  "nativescript": {
    "id": "org.nativescript.mobile",
    "tns-android": {
      "version": "6.1.2"
    }
  },
  "version": "0.0.0",
  "scripts": {
    "sistema": "ng serve --proxy-config proxy.conf.json sistema --open",
    "site": "ng serve --proxy-config proxy.conf.json site --port 9002 --open",
    "public": "ng serve --proxy-config proxy.conf.json public --port 9001 --open",
    "mobile": "ng serve --proxy-config proxy.conf.json mobile --port 9000 --open",
    "build.site.prod": "ng build site --prod",
    "build.sistema.prod": "ng build sistema --prod",
    "build.public.prod": "ng build public --prod",
    "build.mobile.prod": "ng build mobile --prod --configuration=mobile.prod",
    "build.mobile-web.prod": "ng build mobile --prod --output-path='../back-end/src/main/resources/public/sistema/mobile'",
    "build.mobile.dev": "ng build mobile --configuration=mobile.dev",
    "build.all.prod": "npm run build.site.prod && npm run build.sistema.prod && npm run build.public.prod && npm run build.mobile-web.prod"
  },
  "private": true,
