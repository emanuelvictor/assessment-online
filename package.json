{
  "name": "assessment-online",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "web": "ng serve --proxy-config proxy.conf.json -a web --open",
    "debug-web": "node $NODE_DEBUG_OPTION node_modules/.bin/ng serve --proxy-config proxy.conf.json -a web --open",
    "debug-mobile": "node $NODE_DEBUG_OPTION node_modules/.bin/ng serve --proxy-config proxy.conf.json -a mobile --port 9000 --open",
    "mobile": "ng serve --proxy-config proxy.conf.json -a mobile --port 8080 --open",
    "build": "ng build -a web --prod && ng build -a mobile --prod",
    "deploy": "npm run build && firebase deploy",
    "ng-high-mem": "node --max_old_space_size=8192 node_modules/.bin/ng"
  },
  "dependencies": {
    "@angular/animations": "5.1.2",
    "@angular/cdk": "5.0.2",
    "@angular/common": "5.1.2",
    "@angular/compiler": "5.1.2",
    "@angular/core": "5.1.2",
    "@angular/forms": "5.1.2",
    "@angular/http": "5.1.2",
    "@angular/material": "5.0.2",
    "@angular/platform-browser": "5.1.2",
    "@angular/platform-browser-dynamic": "5.1.2",
    "@angular/platform-server": "5.1.2",
    "@angular/router": "5.1.2",
    "@covalent/core": "1.0.0-rc.0",
    "@covalent/highlight": "1.0.0-rc.0",
    "@covalent/markdown": "1.0.0-rc.0",
    "@swimlane/ngx-charts": "^7.1.1",
    "@types/underscore": "^1.8.7",
    "angular2-text-mask": "8.0.4",
    "angularfire2": "5.0.0-rc.6",
    "core-js": "2.5.3",
    "enhanced-resolve": "3.3.0",
    "firebase": "4.10.0",
    "firebase-admin": "~5.8.1",
    "firebase-functions": "0.8.1",
    "hammerjs": "2.0.8",
    "moment": "2.20.1",
    "ng2-currency-mask": "4.4.1",
    "ngx-ellipsis": "^1.0.7",
    "ngx-order-pipe": "^2.0.1",
    "node-sass": "^4.7.2",
    "nprogress": "0.2.0",
    "roboto-fontface": "0.8.0",
    "rxjs": "5.5.6",
    "tslint-angular": "^1.1.0",
    "underscore": "^1.8.3",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.6.2",
    "@angular/compiler-cli": "5.1.2",
    "@types/node": "8.5.2",
    "ts-node": "4.0.2",
    "typescript": "2.4.2",
    "@angular-devkit/build-optimizer": "0.0.36",
    "@angular-devkit/core": "0.0.22",
    "@angular-devkit/schematics": "0.0.42",
    "@ngtools/json-schema": "1.1.0",
    "@ngtools/webpack": "1.9.2",
    "@schematics/angular": "0.1.11",
    "@schematics/schematics": "0.0.11",
    "webpack": "3.10.0"
  }
}
